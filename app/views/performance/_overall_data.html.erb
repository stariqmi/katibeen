        background: -o-linear-gradient(left,  #FD5737 0%,#FD5737 <%=avg%>%,#eeeeee <%=avg%>%,#eeeeee 100%); /* Opera 11.10+ */
        background: -ms-linear-gradient(left,  #FD5737 0%,#FD5737 <%=avg%>%,#eeeeee <%=avg%>%,#eeeeee 100%); /* IE10+ */
        background: linear-gradient(to right,  #FD5737 0%,#FD5737 <%=avg%>%,#eeeeee <%=avg%>%,#eeeeee 100%); /* W3C */    
    }

