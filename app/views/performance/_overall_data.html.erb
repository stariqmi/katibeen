<style>
    

    .circle.overall_fajr {
        <%
            avg = @filteredData[:weeklyPerformedAvgData][:fajr]
        %>
        margin-left: 1%;
        background: #009fe6;
        width: 75px;
        height: 75px;
        border-radius: 50%;
